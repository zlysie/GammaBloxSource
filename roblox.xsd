          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Refs">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:IDREFS">
              <xs:attributeGroup ref="Property"/>
