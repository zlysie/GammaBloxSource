          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Vector3" nillable="true">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="X" nillable="true">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:float">
                    <xs:attributeGroup ref="Referent"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="Y" nillable="true">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:float">
                    <xs:attributeGroup ref="Referent"/>
