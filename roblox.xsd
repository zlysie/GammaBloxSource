                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="R10" nillable="true">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:float">
                    <xs:attributeGroup ref="Referent"/>
